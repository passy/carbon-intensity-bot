{
    "defaultSeverity": "error",
    "extends": [
        "tslint:latest",
        "tslint-config-prettier"
    ],
    "linterOptions": {
        "exclude": [
            "src/*generated.*"
        ]
    },
    "jsRules": {},
    "rules": {
        "no-console": {
            "severity": "off"
        },
        "no-implicit-dependencies": {
            "severity": "off"
        }
    },
    "rulesDirectory": []
}
